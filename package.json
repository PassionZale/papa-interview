{
  "name": "papaen-interview",
  "version": "1.0.0",
  "private": true,
  "description": "papaen interview",
  "main": "index.js",
  "scripts": {
    "app:ddl": "pnpm --filter=@papaen/ddl",
    "ui": "pnpm --filter=@papaen/ui",
